{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=resumegenerator_dev;Username=resumeuser;Password=devpassword123"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    }
  },
  "ClaudeApi": {
    "ApiKey": "your-claude-api-key-here",
    "TimeoutSeconds": 30,
    "RetryAttempts": 2
  },
  "OpenAI": {
    "ApiKey": "your-openai-api-key-here",
    "TimeoutSeconds": 30,
    "RetryAttempts": 2
  },
  "Application": {
    "MaxConcurrentJobs": 2,
    "JobTimeoutMinutes": 10,
    "JobRetentionDays": 7,
    "FailedJobRetentionDays": 3,
    "EnableDetailedLogging": true,
    "EnableJobQueue": true,
    "JobQueuePollingIntervalSeconds": 10
  },
  "RateLimit": {
    "Enabled": false,
    "RequestsPerMinute": 100,
    "MaxConcurrentJobsPerIp": 10
  }
}